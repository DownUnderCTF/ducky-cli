{
  "name": "ducky-cli",
  "description": "A CLI helper for ductf",
  "version": "0.1.0",
  "author": "uint0",
  "bin": {
    "ducky": "./bin/run"
  },
  "bugs": "https://github.com/downunderctf/ducky-cli/issues",
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.0",
    "klaw": "^4.0.1",
    "loglevel": "^1.8.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.1.3",
    "@types/klaw": "^3.0.3",
    "@types/mocha": "^9.0.0",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "globby": "^11",
    "mocha": "^9",
    "mock-fs": "^5.1.2",
    "oclif": "^2.0.0-main.10",
    "pkg": "^5.5.2",
    "prettier": "^2.5.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dirname": "oex",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/downunderctf/ducky-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "bin": "ducky",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "challenge": {
        "description": "Challenge management"
      }
    }
  },
  "repository": "downunderctf/ducky-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"tests/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "pkg-all": "yarn build && pkg --targets node16-linuxstatic,node16-linux,node16-macos,node16-win --compress GZip --out-path distbin/ ."
  },
  "types": "dist/index.d.ts",
  "pkg": {
    "scripts": ["dist/**/*.js"],
    "assets": ["oclif.manifest.json"]
  }
}
